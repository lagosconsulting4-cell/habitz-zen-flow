{
  "installCommand": "cd Landing && npm install && cd ../App && npm install",
  "buildCommand": "cd Landing && npm run build && cd ../App && npm run build && cd .. && node scripts/prepare-dist.cjs",
  "outputDirectory": "dist",
  "cleanUrls": false,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/henrique",
      "destination": "/tiktok?v=henrique.bora",
      "permanent": false
    },
    {
      "source": "/daniel",
      "destination": "/tiktok?v=daniel.almeidaaaa",
      "permanent": false
    },
    {
      "source": "/bia",
      "destination": "/tiktok?v=biaa.ferrariii",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/direct",
      "destination": "/direct/index.html"
    },
    {
      "source": "/tdah/obrigado",
      "destination": "/tdah/obrigado.html"
    },
    {
      "source": "/app/:path((?!.*\\.).*)",
      "destination": "/app/index.html"
    },
    {
      "source": "/antigo/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/:path((?!app|tdah|tiktok|assets|audio|favicon|direct).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/app/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).mp4",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).mp3",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}