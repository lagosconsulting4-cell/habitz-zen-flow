{
  "installCommand": "cd landing && npm install && cd .. && npm install",
  "buildCommand": "cd landing && npm run build && cd .. && npm run build && node scripts/prepare-dist.cjs",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/app/:path((?!.*\\.).*)",
      "destination": "/app/index.html"
    },
    {
      "source": "/app",
      "destination": "/app/index.html"
    }
  ],
  "headers": [
    {
      "source": "/app/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).mp4",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).mp3",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
